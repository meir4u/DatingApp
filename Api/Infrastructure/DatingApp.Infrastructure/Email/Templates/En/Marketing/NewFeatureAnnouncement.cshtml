@{
    ViewData["Title"] = "New Feature Alert!";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
</head>
<body>
    <h1>Explore Our Latest Feature: @Model.FeatureName!</h1>
    <p>Hello @Model.UserName,</p>
    <p>We’re excited to introduce @Model.FeatureName, designed to enhance your experience on our platform. Try it out today!</p>
    <p><a href="@Model.FeatureLink">Learn More</a></p>
</body>
</html>
